<h1>Cran Packages</h1>

<table>
  <thead>
    <tr>
      <th>Package name</th>
      <th>Version</th>
      <th>Title</th>
      <th>Authors</th>
      <th></th>
  </thead>

  <tbody>
    <% @packages.each do |package| %>
      <tr>
        <td><%= package.name %></td>
        <td><%= package.version %></td>
        <td><%= package.title %></td>
        <td><%= package.authors %></td>
        <td><%= link_to 'Show', package %></td>
    <% end %>
  </tbody>
</table>

<br>
